{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "AIKit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "index",
              "quickstart"
            ]
          }
        ]
      },
      {
        "tab": "Core Documentation",
        "groups": [
          {
            "group": "Commands & Usage",
            "pages": [
              "cli-commands",
              "examples"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "configuration",
              "agents"
            ]
          },
          {
            "group": "Package Ecosystem",
            "pages": [
              "packages"
            ]
          }
        ]
      },
      {
        "tab": "Reference",
        "groups": [
          {
            "group": "Troubleshooting",
            "pages": [
              "troubleshooting",
              "development"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/goaikit/aikit",
          "icon": "github"
        },
        {
          "anchor": "Issues",
          "href": "https://github.com/goaikit/aikit/issues",
          "icon": "message"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/goaikit/aikit"
      },
      {
        "label": "Issues",
        "href": "https://github.com/goaikit/aikit/issues"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get AIKit",
      "href": "https://github.com/goaikit/aikit/releases"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/goaikit/aikit"
    }
  }
}
